<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" minHeight="720.0" minWidth="1280.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.buccaneer.MainMenuController">
    <StackPane minHeight="720.0" minWidth="1280.0" prefHeight="150.0" prefWidth="200.0">
        <children>
            <fx:include source="background.fxml" />
            <VBox alignment="CENTER" spacing="20.0">
                <Button fx:id="continueBtn" onAction="#continueBtnAction" text="Continue"/>
                <Button fx:id="newGameBtn" onAction="#newGameBtnAction" text="New Game"/>
                <Button fx:id="quitBtn" onAction="#quitBtnAction" text="Quit"/>
            </VBox>
        </children>
    </StackPane>

</VBox>
